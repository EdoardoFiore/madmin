# MADMIN Environment Configuration
# Copy this file to .env and configure values

# Database (PostgreSQL)
DATABASE_URL=postgresql+asyncpg://madmin:madmin@localhost:5432/madmin

# Security
SECRET_KEY=CHANGE_THIS_TO_A_RANDOM_SECRET_KEY_IN_PRODUCTION
ACCESS_TOKEN_EXPIRE_MINUTES=720

# Server
DEBUG=false
ALLOWED_ORIGINS=*

# Paths
DATA_DIR=/opt/madmin/data
MODULES_DIR=/opt/madmin/backend/modules

# Mock mode for development (don't execute real iptables commands)
MOCK_IPTABLES=false
